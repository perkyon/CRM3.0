            "dev": "vite",
            "build": "vite build && cp sw.js dist/sw.js",
            "vercel-build": "vite build && cp sw.js dist/sw.js",
            "preview": "vite preview",
            "test": "vitest",
            "test:ui": "vitest --ui",
            "test:run": "vitest run",
            "test:coverage": "vitest run --coverage",
            "test:e2e": "playwright test",
            "test:e2e:ui": "playwright test --ui",
            "test:e2e:headed": "playwright test --headed",
            "test:e2e:debug": "playwright test --debug",
            "test:e2e:report": "playwright show-report",
            "test:e2e:prod": "PLAYWRIGHT_BASE_URL=https://crm-3-0-seven.vercel.app playwright test",
            "test:e2e:prod:ui": "PLAYWRIGHT_BASE_URL=https://crm-3-0-seven.vercel.app playwright test --ui",
            "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
            "lint:fix": "eslint src --ext ts,tsx --fix",
            "type-check": "tsc --noEmit",
            "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
            "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
            "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
            "clean": "rm -rf dist node_modules/.vite",
            "postinstall": "echo 'Installation complete'"
      }
